@model MVC_ViewModel_Demo.Models.Category

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Category</h4>
    <hr />
    <div class="row">
        <div class = "col-sm-2">
           Category Id :@Model.Id Name:@Model.Catname
        </div>
        <div class = "col-sm-10">
          @Model.Catname
        </div>
        <div class = "col-sm-2">
          @foreach (var item in Model.products)
          {
              <ul>
                  <li>@item.Id || @item.ProductName|| @item.Price </li>
              </ul>
          }
        </div>
        
    </div>
</div>

