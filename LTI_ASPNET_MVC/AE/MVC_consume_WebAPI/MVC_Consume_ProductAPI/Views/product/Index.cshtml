@model IEnumerable<Product>
<table class="table table-sm table-striped table-bordered m-2">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Price</th>
            <th>Catid</th>
            <th>Update</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var p in Model)
        {
            <tr>
                <td>@p.productId</td>
                <td>@p.ProductName</td>
                <td>@p.price</td>
                <td>@p.catid</td>
                
            </tr>
        }
    </tbody>
</table>